<% include ../partials/head%>
<% include ../partials/header %>

<h1>The Dog Roster</h1>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Breed</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        <% dogOwner.dogs.forEach(d => { %>
            <tr>
                <td><%= d.name %></td>
                <td><%= d.breed %></td>
                <td> <a href="/dogs/<%= d._id%>">link</a></td>
            </tr>
            <% }) %>
        </tbody>
</table>



<% include ../partials/footer %>